{
  "Logging": {
    "LogLevel": {
 "Default": "Information",
      "Microsoft": "Warning",
 "Microsoft.Hosting.Lifetime": "Information"
 },
    "EventLog": {
      "SourceName": "Nimbus Autopilot Telemetry",
 "LogName": "Application"
  }
  },
  "TelemetrySettings": {
    "ApiEndpoint": "http://localhost:5000",
    "ApiKey": "your_api_key_here",
 "DeploymentProfile": "Standard",
    "IntervalSeconds": 30,
    "HeartbeatIntervalSeconds": 300,
    "MaxRetries": 3
  }
}
